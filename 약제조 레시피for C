#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
#include <math.h>

typedef struct {
    bool kidney_blood_flow_normal;
    bool diencephalon_flow;
    bool filtered_components;
    bool hormone_levels;
    bool rbc_in_lung;
    bool wbc_mixed;
    bool brain_reverse_flow;
    bool protein_intake;
    int rbc_wbc_ratio;
    bool mineral_intake;
    bool rehabilitation_needed;
    bool neural_disorder;
} PatientData;

char* check_conditions(PatientData data) {
    if (!data.kidney_blood_flow_normal)
        return "콩팥과 달팽이관 간 모세혈관 혈액순환 이상";
    if (!data.diencephalon_flow)
        return "뇌수 흐름 방향 이상";
    return "정상";
}

char* check_circulatory_and_digestive_system(PatientData data) {
    if (!data.filtered_components)
        return "간에서 여과된 성분이 췌장으로 전달되지 않음";
    if (!data.hormone_levels)
        return "대뇌 호르몬 수치 이상";
    return "정상";
}

